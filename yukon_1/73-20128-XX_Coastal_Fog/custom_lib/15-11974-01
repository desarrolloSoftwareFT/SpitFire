! Testjet Testable:  no
! JTAG Device:       no
! Fault coverage:    Analog Functional
! Constraints:

test powered analog
  power     pins 13,22,26,29
  nonanalog pins 1,2,4,7,8,25,27,28

  test "vin"
  test "vtt"
  test "vtts"
  test "bst"
  test "lx"
  test "dh"
  test "dl"
  test "refin"
  test "ref"
  test "pok1"
  test "pok2"
  test "ss"
  test "fb"
  test "out"
end test

! **********************************************************

subtest "vin"

 connect i to pins 17
 connect l to pins 24

 detector dcv, expect 1.0  ! Modify during debug
 measure 12*1.05, 12*0.95
end subtest

subtest "vtt"

 connect i to pins 12
 connect l to pins 24

 detector dcv, expect 0.75  ! Modify during debug
 measure 0.75*1.05, 0.75*0.95
end subtest

subtest "vtts"

 connect i to pins 9
 connect l to pins 23

 detector dcv, expect 0.75  ! Modify during debug
 measure 0.75*1.05, 0.75*0.95
end subtest

