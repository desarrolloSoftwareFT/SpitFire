!!!!    6    0    1  909988989  V5a18                                         

!    74157
!    Multiplexer, 1-Line of 2-Line, Quad
!    revision A.01.00

combinatorial

assign   VCC to pins 16
assign   GND to pins 8

assign   E1_Data_Inputs to pins 3,2
assign   E2_Data_Inputs to pins 6,5
assign   E3_Data_Inputs to pins 13,14
assign   E4_Data_Inputs to pins 10,11

assign   Device_Enable  to pins 15
assign   Select_Input   to pins 1

assign   E1_Output to pins 4
assign   E2_Output to pins 7
assign   E3_Output to pins 12
assign   E4_Output to pins 9

power    VCC, GND

family   LVT

inputs   E1_Data_Inputs,E2_Data_Inputs,E3_Data_Inputs,E4_Data_Inputs
inputs   Device_Enable, Select_Input

outputs  E1_Output,E2_Output,E3_Output,E4_Output

trace E1_Output   to E1_Data_Inputs
trace E2_Output   to E2_Data_Inputs
trace E3_Output   to E3_Data_Inputs
trace E4_Output   to E4_Data_Inputs
trace E1_Output,E2_Output,E3_Output,E4_Output to Device_Enable, Select_Input

!***************************************************************************
!***************************************************************************

vector  E1_Enabled__Inputs_000
   set   Device_Enable  to "0"
   set   Select_Input   to "0"
   set   E1_Data_Inputs to "00"
   set   E1_Output      to "0"
end vector

vector  E1_Enabled__Inputs_001
   set   Device_Enable  to "0"
   set   Select_Input   to "0"
   set   E1_Data_Inputs to "01"
   set   E1_Output      to "1"
end vector

