!    Each element tested separately. With the strobe input enabled, the
!    select line and the two inputs for a given element are cycled
!    through all eight possible combinations. Then strobe is checked with
!    the last pattern for the SA0 condition.

unit  "Multiplexer, 2-Input, Element1"
   execute  E1_Enabled__Inputs_000
   execute  E1_Enabled__Inputs_001
   execute  E1_Enabled__Inputs_010
   execute  E1_Enabled__Inputs_011
   execute  E1_Enabled__Inputs_100
   execute  E1_Enabled__Inputs_101
   execute  E1_Enabled__Inputs_110
   execute  E1_Enabled__Inputs_111

   execute  E1_Strobe_False
end unit

unit  "Multiplexer, 2-Input, Element2"
   execute  E2_Enabled__Inputs_000
   execute  E2_Enabled__Inputs_001
   execute  E2_Enabled__Inputs_010
   execute  E2_Enabled__Inputs_011
   execute  E2_Enabled__Inputs_100
   execute  E2_Enabled__Inputs_101
   execute  E2_Enabled__Inputs_110
   execute  E2_Enabled__Inputs_111

   execute  E2_Strobe_False
end unit

unit  "Multiplexer, 2-Input, Element3"
   execute  E3_Enabled__Inputs_000
   execute  E3_Enabled__Inputs_001
   execute  E3_Enabled__Inputs_010
   execute  E3_Enabled__Inputs_011
   execute  E3_Enabled__Inputs_100
   execute  E3_Enabled__Inputs_101
   execute  E3_Enabled__Inputs_110
   execute  E3_Enabled__Inputs_111

   execute  E3_Strobe_False
end unit

unit  "Multiplexer, 2-Input, Element4"
   execute  E4_Enabled__Inputs_000
   execute  E4_Enabled__Inputs_001
   execute  E4_Enabled__Inputs_010
   execute  E4_Enabled__Inputs_011
   execute  E4_Enabled__Inputs_100
   execute  E4_Enabled__Inputs_101
   execute  E4_Enabled__Inputs_110
   execute  E4_Enabled__Inputs_111

   execute  E4_Strobe_False
end unit

