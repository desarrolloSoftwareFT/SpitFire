test powered analog
 disconnect all
 test "PIN3"
 test "PIN4"
 test "PIN5"
 test "PIN6" 
end test

subtest "PIN3"

    connect i to pins 3
    connect l to pins 1

    detector dcv, expect 1
    measure 13,11
end subtest

subtest "PIN4"

    connect i to pins 4 
    connect l to pins 1

    detector dcv, expect 1
    measure 13,11
end subtest

subtest "PIN5"

    connect i to pins 5 
    connect l to pins 2

    detector dcv, expect 1
    measure 13,11
end subtest

subtest "PIN6"

    connect i to pins 6 
    connect l to pins 2

    detector dcv, expect 1
    measure 0.5,-0.5
end subtest
