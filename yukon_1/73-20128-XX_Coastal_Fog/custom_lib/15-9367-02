!!!!    6    0    1  591688800   0000                                         

!     74LS244
!     Buffer  3-state low_true_enable  octal
!    revision A.01.00

combinatorial

assign   VCC      to pins 20
assign   GND      to pins 10

assign   E1_Input       to pins 2
assign   E2_Input       to pins 4
assign   E3_Input       to pins 6
assign   E4_Input       to pins 8

assign   E1_4_Enable_bar  to pins 1

assign   E1_Output      to pins 18
assign   E2_Output      to pins 16
assign   E3_Output      to pins 14
assign   E4_Output      to pins 12

assign   E5_Input       to pins 17
assign   E6_Input       to pins 15
assign   E7_Input       to pins 13
assign   E8_Input       to pins 11

assign   E5_8_Enable_bar  to pins 19

assign   E5_Output      to pins 3
assign   E6_Output      to pins 5
assign   E7_Output      to pins 7
assign   E8_Output      to pins 9

power    VCC, GND

family   TTL

inputs   E1_Input, E2_Input, E3_Input, E4_Input, E1_4_Enable_bar
inputs   E5_Input, E6_Input, E7_Input, E8_Input, E5_8_Enable_bar

outputs  E1_Output, E2_Output, E3_Output, E4_Output
outputs  E5_Output, E6_Output, E7_Output, E8_Output

disable E1_Output  with  E1_4_Enable_bar  to  "1"
disable E2_Output  with  E1_4_Enable_bar  to  "1"
disable E3_Output  with  E1_4_Enable_bar  to  "1"
disable E4_Output  with  E1_4_Enable_bar  to  "1"

disable E5_Output  with  E5_8_Enable_bar  to  "1"
disable E6_Output  with  E5_8_Enable_bar  to  "1"
disable E7_Output  with  E5_8_Enable_bar  to  "1"
disable E8_Output  with  E5_8_Enable_bar  to  "1"

when E1_4_Enable_bar is "1"   inactive E1_Output, E2_Output
when E1_4_Enable_bar is "1"   inactive E3_Output, E4_Output
when E5_8_Enable_bar is "1"   inactive E5_Output, E6_Output
when E5_8_Enable_bar is "1"   inactive E7_Output, E8_Output

trace E1_Output   to E1_Input
trace E2_Output   to E2_Input
trace E3_Output   to E3_Input
trace E4_Output   to E4_Input
trace E5_Output   to E5_Input
trace E6_Output   to E6_Input
trace E7_Output   to E7_Input
trace E8_Output   to E8_Input
trace E1_Output, E2_Output, E3_Output, E4_Output   to E1_4_Enable_bar
trace E5_Output, E6_Output, E7_Output, E8_Output   to E5_8_Enable_bar

!***************************************************************************
!***************************************************************************

vector  E1_Low__D
   set   E1_4_Enable_bar   to "0"
   set   E5_8_Enable_bar   to "1"
   set   E1_Input          to "0"
   set   E1_Output         to "0"
end vector

vector  E1_High__D
   set   E1_4_Enable_bar   to "0"
   set   E5_8_Enable_bar   to "1"
   set   E1_Input          to "1"
   set   E1_Output         to "1"
end vector

