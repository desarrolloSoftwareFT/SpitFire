    (BIDIR_OUT,INTEST,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );


  -- System Bidirectional, Used as Component-Test Output
  constant BSR_BIDIR_TO              : CELL_INFO :=
   ((BIDIR_IN, EXTEST,  PI ),
    (BIDIR_IN, SAMPLE,  PI ),
    (BIDIR_IN, INTEST,  X  ),
    (BIDIR_OUT,EXTEST,  PO ),
    (BIDIR_OUT,SAMPLE,  PI ),
    (BIDIR_OUT,INTEST,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );

  constant BSR_BIDIR_TO_OO           : CELL_INFO :=
   ((BIDIR_IN, EXTEST,  PI ),
    (BIDIR_IN, SAMPLE,  PI ),
    (BIDIR_IN, INTEST,  X  ),
    (BIDIR_OUT,EXTEST,  PO ),
    (BIDIR_OUT,SAMPLE,  PI ),
    (BIDIR_OUT,INTEST,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );


  -- Enable Cell for 3-State I/Os, Not Used as Component-Test Pin
  constant BSR_ENAB_NT               : CELL_INFO :=
   ((CONTROL,  EXTEST,  UPD),
    (CONTROL,  SAMPLE,  PI ),
    (CONTROL,  INTEST,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );

  constant BSR_ENABR_NT              : CELL_INFO :=
   ((CONTROLR, EXTEST,  UPD),
    (CONTROLR, SAMPLE,  PI ),
    (CONTROLR, INTEST,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );


      -- Observe-only Boundary Scan Cells

  -- System Input, Observe-Only, Not Used as Component-Test Pin
  -- INTEST not supported
  constant BSR_IN_NT_OBS             : CELL_INFO :=
   ((INPUT,    EXTEST,  PI),
    (INPUT,    SAMPLE,  PI),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ) );


