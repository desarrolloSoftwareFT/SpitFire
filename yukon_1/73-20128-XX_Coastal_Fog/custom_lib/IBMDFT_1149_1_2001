  constant BSR_BIDIR_OBS             : CELL_INFO :=
   ((BIDIR_IN, EXTEST,  PI ),
    (BIDIR_IN, SAMPLE,  PI ),
    (BIDIR_OUT,EXTEST,  PO ),
    (BIDIR_OUT,SAMPLE,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ) );


  -- System Bidirectional, Observe-Only, Used as Component-Test Output
  -- INTEST not supported
  constant BSR_BIDIR_TO_OBS          : CELL_INFO :=
   ((BIDIR_IN, EXTEST,  PI ),
    (BIDIR_IN, SAMPLE,  PI ),
    (BIDIR_OUT,EXTEST,  PO ),
    (BIDIR_OUT,SAMPLE,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ) );

  constant BSR_BIDIR_TO_OO_OBS       : CELL_INFO :=
   ((BIDIR_IN, EXTEST,  PI ),
    (BIDIR_IN, SAMPLE,  PI ),
    (BIDIR_OUT,EXTEST,  PO ),
    (BIDIR_OUT,SAMPLE,  PI ),
    (INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ) );


  -- Dummy Boundary Scan Cell
  constant BSR_DUMMY                         : CELL_INFO :=
   ((INTERNAL, EXTEST,  X  ),
    (INTERNAL, SAMPLE,  X  ),
    (INTERNAL, INTEST,  X  ) );


  -- System Input, Not Used as Component-Test Pin,  AC JTAG
  constant AC_BSR_IN_NT                 : CELL_INFO :=
   ((INPUT,    EXTEST,  PI ),
    (INPUT,    SAMPLE,  PI ),
    (INPUT,    INTEST,  CAP),
